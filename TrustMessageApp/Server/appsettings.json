{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=SecureAppDb;Trusted_Connection=True;MultipleActiveResultSets=true;"
  },
  "KeyManagement": {
    "Key": "${MASTER_KEY}",
    "IV": "${MASTER_IV}"
  }
}
